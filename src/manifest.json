{
  "name": "dot-shield",
  "version": "0.1.0",
  "description": "Dot Shield: A fast and simple ad blocker for dot browser",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": ["blockedDomains.js", "background.js"]
  },
  "manifest_version": 2
}

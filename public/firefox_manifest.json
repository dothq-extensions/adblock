{
  "name": "Dot Shield",
  "version": "0.1.0",
  "description": "Dot Shield: A fast and simple ad blocker for dot browser",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/",
    "https://*/"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": "images/logo/shield.svg",
    "default_title": "Dot Shield",
    "default_popup": "popup.html"
  }
}
